public class lesson2 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
 
        System.out.println("Введите число:");
        int num1 = in.nextInt();
     
        int num2 = 3;
        int num3 = num1 % num2;
        
 
     if(num3 == 0){
         System.out.println("Число делиться без остатка ");
 
    
    }else if(num3 != 0 ){
            System.out.println("Число делиться с остатком ");
     }
 
    }
}
