public class lesson6 {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
 
        System.out.println("Введите число:");
        int num1 = in.nextInt();
     
        int num2 = num1 / 8;
        int num3 = num2/10;
        System.out.println(num3 = num2 / 8);
 
    
         }
 
}
